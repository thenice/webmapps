<h1>Listing layer_filters</h1>

<table>
  <tr>
    <th>Layer</th>
    <th>Column name</th>
    <th>Operation</th>
    <th>Value</th>
    <th>State</th>
  </tr>

<% for layer_filter in @layer_filters %>
  <tr>
    <td><%=h layer_filter.layer_id %></td>
    <td><%=h layer_filter.column_name %></td>
    <td><%=h layer_filter.operation %></td>
    <td><%=h layer_filter.value %></td>
    <td><%=h layer_filter.state %></td>
    <td><%= link_to 'Show', layer_filter %></td>
    <td><%= link_to 'Edit', edit_layer_filter_path(layer_filter) %></td>
    <td><%= link_to 'Destroy', layer_filter, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New layer_filter', new_layer_filter_path %>
